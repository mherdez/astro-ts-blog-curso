---
interface Usuario {
  first_name: string;
  last_name: string;
  email: string;
  avatar: string;
}

const response = await fetch('https://reqres.in/api/users?page=2');
const { data } = await response.json();
console.log(data)
---

{
  data.map(({avatar, first_name, last_name, email}: Usuario) => {
    return (
      <div>
        <img src={avatar} alt={`${first_name} ${last_name}`} />
        <h1>
          {first_name}
          {last_name}
        </h1>
        <p>{email}</p>
      </div>
    );
  })
}
